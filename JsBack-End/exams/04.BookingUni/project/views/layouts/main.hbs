<html lang="en">

	<head>
		<meta charset="UTF-8" />
		<title>BookingUni</title>
		<link rel="stylesheet" href="/static/css/style.css" />
	</head>

	<body>

		<div id="container">
			{{#if errors}}

				<div class="notification errorBox">
					{{#each errors}}
						<p> {{this}} </p>
					{{/each}}
				</div>
			{{/if}}
			{{#if user}}
				<nav>
					<div class="left-container">
						<ul>
							<li><a href="/">Home</a></li>
							<li><a href="/hotel/booking/create">Add +</a></li>
						</ul>
					</div>
					<div class="right-container">
						<a
							href="/user/profile"
							class="log-out"
						>{{user.email}}</a>
						<a href="/user/logout" class="log-out">Logout</a>
					</div>
				</nav>
			{{else}}
				<nav>
					<div class="left-container">
						<ul>
							<li><a href="/">Home</a></li>
							<li><a href="/user/login">Login</a></li>
							<li><a href="/user/register">Register</a></li>
						</ul>
					</div>
				</nav>
			{{/if}}
			{{{body}}}

			<footer>@SoftUni - JS Back-end - BookingUni 2020</footer>
		</div>

	</body>

</html>